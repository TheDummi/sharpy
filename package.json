{
	"name": "sharpyy",
	"version": "1.0.3",
	"type": "module",
	"main": "./dist/main.cjs",
	"module": "./dist/main.mjs",
	"exports": {
		"require": {
			"types": "./dist/main.d.cts",
			"default": "./dist/main.cjs"
		},
		"import": {
			"types": "./dist/main.d.mts",
			"default": "./dist/main.mjs"
		}
	},
	"scripts": {
		"dev": "pkgroll --watch --tsconfig=tsconfig.json --clean-dist"
	},
	"keywords": [
		"windows",
		"terminal",
		"colors",
		"color",
		"pretty"
	],
	"author": "Dummi",
	"license": "MIT",
	"description": "A package that allows windows to have pretty colors in the terminal.",
	"devDependencies": {
		"pkgroll": "^2.4.2",
		"prettier": "^3.3.3",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"terminal-link": "2.1.1"
	}
}
